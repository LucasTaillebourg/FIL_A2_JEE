<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.1"
	xmlns="http://xmlns.jcp.org/xml/ns/persistence"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/persistence http://xmlns.jcp.org/xml/ns/persistence/persistence_2_1.xsd">
	<persistence-unit name="subscriberBDD"
		transaction-type="RESOURCE_LOCAL">
		<provider>org.eclipse.persistence.jpa.PersistenceProvider</provider>
		<class>com.jee.subscriberBDD.modele.City</class>
		<class>com.jee.subscriberBDD.modele.Country</class>
		<class>com.jee.subscriberBDD.modele.Measure</class>
		<class>com.jee.subscriberBDD.modele.Sensor</class>
		<class>com.jee.subscriberBDD.modele.User</class>
		<properties>
			<property name="javax.persistence.jdbc.url"
				value="jdbc:postgresql://localhost:5432/databaseJEE" />
			<property name="javax.persistence.jdbc.driver"
				value="org.postgresql.Driver" />
			<property name="javax.persistence.jdbc.user"
				value="postgres" />
			<property name="javax.persistence.jdbc.password"
				value="1234" />
			<property name="eclipselink.logging.level" value="ALL" />
		</properties>
	</persistence-unit>
</persistence>
